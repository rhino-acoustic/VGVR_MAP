{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node",
      "config": {
        "maxDuration": 300
      }
    }
  ],
  "routes": [
    {
      "src": "/api/png-files",
      "dest": "/api/png-files.js"
    },
    {
      "src": "/api/generate-from-sheets",
      "dest": "/api/generate-from-sheets.js"
    },
    {
      "src": "/api/generate-images",
      "dest": "/api/generate-images.js"
    },
    {
      "src": "/generated-png/(.*)",
      "dest": "/generated-png/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/server.js"
    }
  ]
}